{
  "name": "es6-react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "a Webpack boilerplate with es6 && hot reloading React components, and error handling on module and component level.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.dev.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyygtfx/es6-react-webpack-boilerplate.git"
  },
  "keywords": [
    "es6",
    "react",
    "webpack"
  ],
  "author": "Go7hic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dyygtfx/es6-react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/dyygtfx/es6-react-webpack-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
